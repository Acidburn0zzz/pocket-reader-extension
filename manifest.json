{
    "name": "New Tab - Pocket",
    "version": "1.1.1",
    "manifest_version": 2,
    "key": "",
    "description": "New tab page for all your Pocket links",
    "author": "Gert Gl√ºkmann @glukmann",
    "homepage_url": "http://gglukmann.github.io/pocket",
    "icons": {
        "16": "icon16.png",
        "128": "icon128.png",
        "256": "icon256.png"
    },
    "browser_action": {
        "default_icon": "icon16.png"
    },
    "chrome_url_overrides" : {
        "newtab": "page.html"
    },
    "content_scripts": [{
        "js": [ "app.js" ],
        "matches": [ "https://*.getpocket.com/*"]
    }],
    "permissions": [
        "identity",
        "tabs",
        "https://*.getpocket.com/*"
    ]
}
